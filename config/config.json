                    "name": "__user_token",
                    "httponly": true,
                    "expires": 604800
                },
                "lang": {
                    "name": "__user_lang",
                    "httponly": false,
                    "expires": 31536000
                },
                "smartbanner": {
                    "name": "__user_smartbanner",
                    "httponly": false,
                    "expires": 604800
                }
            }
        },
        "option": {
            "path": "/",
            "secure": true,
            "sameSite": "strict"
        }
